        tc_data.packet_id = tc_handler->packet_header.packet_id;
        tc_data.packet_error_ctrl = tc_handler->packet_error_ctrl;
        tc_data.tc_num_bytes = tc_handler->tc_descriptor.tc_num_bytes;

        *subtype = tc_handler->df_header.subtype;

        if (*subtype == 1 || *subtype == 2 || *subtype == 6){

            var tc_data_1_2_6_aux : PS12TC_1_2_6_Data = {
                N = 0,
                PMONID = 0
            };

           
            *result = tc_handler_get_u8_appdata_field(tc_handler, &mut (tc_data_1_2_6_aux.N));
            *result = tc_handler_get_u16_appdata_field(tc_handler, &mut (tc_data_1_2_6_aux.PMONID));

            tc_data.tc_data_1_2_6 = tc_data_1_2_6_aux;

        } else if (*subtype == 5){

            var mon_config_aux : ParamMonitoringConfiguration = {
                    PID = 0,
                    type = MonitorCheckType::Free,
                    enabled = false,
                    definition = MonitorDefinition::Unselected,
                    repetition = 0,
                    repetition_control = 0,
                    interval = 0,
                    interval_control = 0,
                    transition_obt = {tv_sec = 0, tv_usec = 0},
                    temp_state = CheckState::Unselected,
                    current_state = CheckState::Unselected
            };
            var aux : u8 = 0;

            var tc_data_5_aux : PS12TC_5_Data = {
                N = 0,
                PMONID = 0,
                mon_config = mon_config_aux
            };

            *result = tc_handler_get_u8_appdata_field(tc_handler, &mut (tc_data_5_aux.N));
            *result = tc_handler_get_u16_appdata_field(tc_handler, &mut (tc_data_5_aux.PMONID));
            *result = tc_handler_get_u16_appdata_field(tc_handler, &mut tc_data_5_aux.mon_config.PID);
            *result = tc_handler_get_u8_appdata_field(tc_handler, &mut tc_data_5_aux.mon_config.interval);
            *result = tc_handler_get_u8_appdata_field(tc_handler, &mut tc_data_5_aux.mon_config.repetition);
            *result = tc_handler_get_u8_appdata_field(tc_handler, &mut aux);
            tc_data_5_aux.mon_config.type = get_check_type(aux);

            if (self->exect_tc_req_status_update.tc_data_5.mon_config.type is MonitorCheckType::ExpectedValue) {

                tc_data_5_aux.mon_config.definition = self->get_PMON_value_check_definition(tc_handler, result);

            }
            else if (self->exect_tc_req_status_update.tc_data_5.mon_config.type is MonitorCheckType::Limits){

                tc_data_5_aux.mon_config.definition = self->get_PMON_limit_check_definition(tc_handler, result);
            }
            else {
                //Error
            }

            tc_data.tc_data_5 = tc_data_5_aux;

        } else {

            // Invalid subtype

        }

        return tc_data;

    }

    procedure exec_tc (&mut self, tc_handler : &mut TCHandlerT, result: &mut Result){

        var subtype : u8 = 0;

        for i : usize in 0 .. 3 while (self->exect_tc_req_status is PS12ExecTCReqStatus::Exit == false) {

            match self->exect_tc_req_status {

                case Init => {

                    self->exect_tc_req_status_update = self->get_TC_params(tc_handler, &mut subtype, result);
                    if (*result is Result::Error) {
                        self->exect_tc_req_status = self->manage_short_pack_length_error();
                    } else {
                        self->exect_tc_req_status = PS12ExecTCReqStatus::ExecTC;
                    }

                }
                case ExecTC => {

                    if (subtype == 1) {

                        self->exect_tc_req_status = self->exec12_1TC();

                    } else if (subtype == 2) {

                        self->exect_tc_req_status = self->exec12_2TC();

                    } else if (subtype == 5) {

                        self->exect_tc_req_status = self->exec12_5TC();

                    } else if (subtype == 6) {

                        self->exect_tc_req_status = self->exec12_6TC();

                    } else {

                        self->exect_tc_req_status = PS12ExecTCReqStatus::Error;
                    }
                }

                case Error => {

                    *result = Result::Error;
                    self->exect_tc_req_status = PS12ExecTCReqStatus::Exit;

                }
                case Exit => {
                    //Unreachable point
                }  
            }
        }

        if(self->exect_tc_req_status is PS12ExecTCReqStatus::Exit){

            self->exect_tc_req_status = PS12ExecTCReqStatus::Init;
        }

        return;
    
    }

     
};




