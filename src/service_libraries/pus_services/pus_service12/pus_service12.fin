    viewer get_PMON_limit_check_definition (&self, tc_handler : &mut TCHandlerT, result: &mut Result) -> MonitorDefinition {

        var limits_def : ParamLimitCheckDefinition = {
            low_limit = 0,
            low_limit_evID = 0,
            high_limit = 0,
            high_limit_evID = 0
        };

        var type : DataPoolItemType = sys_data_pool_get_item_type(self->exect_tc_req_status_update.tc_data_5.mon_config.PID);

        match type {

            case u8_t => {

                var low_limit : u8 = 0;
                var high_limit : u8 = 0;

                //get low limit
                *result = tc_handler_get_u8_appdata_field(tc_handler, &mut low_limit);

                //get low limit evID
                *result = tc_handler_get_u16_appdata_field(tc_handler, &mut (limits_def.low_limit_evID));

                //get high limit
                *result = tc_handler_get_u8_appdata_field(tc_handler, &mut high_limit);

                //get high limit evID
                *result = tc_handler_get_u16_appdata_field(tc_handler, &mut (limits_def.high_limit_evID));

                limits_def.low_limit = low_limit as u32;
                limits_def.high_limit = high_limit as u32;

            }
            case u32_t => {

                //get low limit
                *result = tc_handler_get_u32_appdata_field(tc_handler, &mut (limits_def.low_limit));
                
                //get low limit evID
                *result = tc_handler_get_u16_appdata_field(tc_handler, &mut (limits_def.low_limit_evID));

                //get high limit
                *result = tc_handler_get_u32_appdata_field(tc_handler, &mut (limits_def.low_limit));

                //get high limit evID
                *result = tc_handler_get_u16_appdata_field(tc_handler, &mut (limits_def.high_limit_evID));

            }
            case NoValidPID => {
                *result = Result::Error;
            }


        }

        var param_limit_check_definition : MonitorDefinition = MonitorDefinition::ParamLimitCheck(limits_def);
        
        return param_limit_check_definition;

    }


