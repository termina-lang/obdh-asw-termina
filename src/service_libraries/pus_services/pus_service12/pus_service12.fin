                    }
                }

                if(self->manage_new_status()){

                    self->req_status_update.event_triggered = true;
                    self->req_status_update.fault_info = ParamOutOfLimit(fault_info);
                    self->add_monitoring_transition();
                    self->req_status_update.EvID = check_definition.high_limit_evID;
                }

            }
            case ParamDeltaCheck(check_definition) => {

            }
            case Unselected => {
                
            }
        }

        return next_status;
    }


