<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE tm_tc_format SYSTEM "../../../DTDs/tm_tc_format.dtd">

<tm_tc_format protocol="ASW_1.4 Start Execution Error (8-bit and 32-bit error data)" type="TM">
	<CSField fid="0" pfid="0" name="SourceData" type="structured" byteOrder="big_endian" firstBit="MSB">
		<size bytes="10" bits="0" />			<!-- size in bits = 9*8 -->
		<globalOffset bytes="0" bits="0" />		<!-- offset in bits = 0 -->
	</CSField>

	<CSField fid="1" pfid="0" name="PacketID" type="uint" byteOrder="big_endian" firstBit="MSB">
		<size bytes="2" bits="0" />				<!-- size in bits = 16 -->
		<globalOffset bytes="0" bits="0" />		<!-- offset in bits = 0 -->
	</CSField>

	<CSField fid="2" pfid="0" name="PacketSequenceControl" type="uint" byteOrder="big_endian" firstBit="MSB">
		<size bytes="2" bits="0" />				<!-- size in bits = 16 -->
		<globalOffset bytes="2" bits="0" />		<!-- offset in bits = 8 -->
	</CSField>

	<CSField fid="3" pfid="0" name="Error_Code" type="uint" byteOrder="big_endian" firstBit="MSB">
		<size bytes="1" bits="0" />				<!-- size in bits = 8 -->
		<globalOffset bytes="4" bits="0" />		<!-- offset in bits = 0 -->
	</CSField>

	<CSField fid="4" pfid="0" name="Error_Data1" type="uint" byteOrder="big_endian" firstBit="MSB">
		<size bytes="1" bits="0" />				<!-- size in bits = 8 -->
		<globalOffset bytes="5" bits="0" />		<!-- offset in bits = 8 -->
	</CSField>

	<CSField fid="5" pfid="0" name="Error_Data2" type="uint" byteOrder="big_endian" firstBit="MSB">
		<size bytes="4" bits="0" />				<!-- size in bits = 32 -->
		<globalOffset bytes="6" bits="0" />		<!-- offset in bits = 48 -->
	</CSField>
	

</tm_tc_format>
