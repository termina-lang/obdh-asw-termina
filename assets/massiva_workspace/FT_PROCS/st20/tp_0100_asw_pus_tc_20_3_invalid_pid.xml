<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE test_proc SYSTEM "../DTDs/test_proc.dtd">

<!--

ID: TP_0100_ASW_PUS_TC_20_3_INVALID_PID
Description:

The test checks that telecommand TC(20,3) with invalid Parameter ID is
effectively rejected and the TM (1,1), and TM (1,4) with error code 22 (invalid
Parameter ID), are received.

-->

<test_proc name="TP_0100_ASW_PUS_TC_20_3_INVALID_PID">
	<step name="tc_20_3_invalid_pid" id="0" prev_step_idref="NULL" output_idref_from_prev_step="NULL" mode="continuous">
		<inputs>
			<input_level_2 name="tc_20_1" ifRef="0" delay_value="120" delay_unit="miliseconds">
				<level2 format="TC_Formats/Level2/st20/asw_pus_tc_20_3_uint32_format.xml"/>
				<app_to_level2 export="STEPS_XML/INPUTS/tc_20_3/tc_20_3_set_invalid_pid.xml"/>
				<level1 format="DEFAULT"/>
				<level2_to_level1 export="TC_Formats/Lv2_To_Lv1/st20/asw_pus_tc_20_3_uint32_to_asw_pus_tc_ack_0b1001.xml"/>
				<level0 format="DEFAULT"/>
				<level1_to_level0 export="DEFAULT"/>
			</input_level_2>
		</inputs>
		<outputs checkmode="all" valid_time_interval_value="500" valid_time_interval_unit="miliseconds">
			<output_level_1 name="tm_1_1" id="0" ifRef="0">
				<level1 format="DEFAULT"/>
				<level1_filter apply_def_filter="yes" extra_filter="TM_Filters/Level1/st01/asw_pus_v2_tm_1_1_filter.xml"/>
				<level0 format="DEFAULT"/>
				<level1_from_level0 import="DEFAULT"/>
				<level0_filter apply_def_filter="yes" extra_filter="NULL"/>
			</output_level_1>
			<output_level_2 name="tm_1_4" id="1" ifRef="0">
				<level2 format="TM_Formats/Level2/st01/asw_pus_v2_tm_1_4_error_uint16_data.xml"/>
				<level2_filter apply_def_filter="no" extra_filter="TM_Filters/Level2/st01/asw_pus_tm_1_4_error_22_pid_80.xml"/>
				<level1 format="DEFAULT"/>
 				<level2_from_level1 import="TM_Formats/Lv2_From_Lv1/st01/asw_pus_tm_1_4_from_asw_pus_v2_tm.xml"/>
				<level1_filter apply_def_filter="yes" extra_filter="TM_Filters/Level1/st01/asw_pus_v2_tm_1_4_filter.xml"/>
				<level0 format="DEFAULT"/>
				<level1_from_level0 import="DEFAULT"/>
				<level0_filter apply_def_filter="yes" extra_filter="NULL"/>
			</output_level_2>
		</outputs>			
	</step>
</test_proc>
