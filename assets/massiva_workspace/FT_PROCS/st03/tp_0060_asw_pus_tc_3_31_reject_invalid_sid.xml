<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE test_proc SYSTEM "../../DTDs/test_proc.dtd">

<!--

ID: TP_0060_ASW_PUS_TC_3_31_REJECT_INVALID_SID
Description:

The test checks that telecommand TC(3,31) with invalid SID is effectively
rejected and the TM (1,1) and TM (1,4) are received with error code 5 (invalid
SID).

-->

<test_proc name="TP_0060_ASW_PUS_TC_3_31_REJECT_INVALID_SID">
	<step name="tc_3_31_invalid_sid" id="0" prev_step_idref="NULL" output_idref_from_prev_step="NULL" mode="continuous">
		<inputs>
			
			<input_level_2 name="tc_3_31" ifRef="0" delay_value="100" delay_unit="miliseconds">
				<level2 format="TC_Formats/Level2/st03/asw_pus_tc_3_31_format.xml"/>
				<app_to_level2 export="STEPS_XML/INPUTS/tc_3_31/tc_3_31_set_hk_period_invalid_sid.xml"/>
				<level1 format="DEFAULT"/>
				<level2_to_level1 export="TC_Formats/Lv2_To_Lv1/st03/asw_pus_tc_3_31_to_asw_pus_v2_tc_ack_0b1001.xml"/>
				<level0 format="DEFAULT"/>
				<level1_to_level0 export="DEFAULT"/>
			</input_level_2>
		
		</inputs>
		<outputs checkmode="all" valid_time_interval_value="500" valid_time_interval_unit="miliseconds">
			<output_level_1 name="tm_1_1" id="0" ifRef="0">
				<level1 format="DEFAULT"/>
				<level1_filter apply_def_filter="yes" extra_filter="TM_Filters/Level1/st01/asw_pus_v2_tm_1_1_filter.xml"/>
				<level0 format="DEFAULT"/>
				<level1_from_level0 import="DEFAULT"/>
				<level0_filter apply_def_filter="yes" extra_filter="NULL"/>
			</output_level_1>
			<output_level_2 name="tm_1_4" id="1" ifRef="0">
				<level2 format="TM_Formats/Level2/st01/asw_pus_v2_tm_1_4_error_uint16_data.xml"/>
				<level2_filter apply_def_filter="no" extra_filter="TM_Filters/Level2/st01/asw_pus_tm_1_4_error_5_sid_0F.xml"/>
				<level1 format="DEFAULT"/>
 				<level2_from_level1 import="TM_Formats/Lv2_From_Lv1/st01/asw_pus_tm_1_4_from_asw_pus_v2_tm.xml"/>
				<level1_filter apply_def_filter="yes" extra_filter="TM_Filters/Level1/st01/asw_pus_v2_tm_1_4_filter.xml"/>
				<level0 format="DEFAULT"/>
				<level1_from_level0 import="DEFAULT"/>
				<level0_filter apply_def_filter="yes" extra_filter="NULL"/>
			</output_level_2>
		</outputs>			
	</step>
</test_proc>
